
<form action="/tm/projectselected" method="POST" autocomplete="off">
 <input type="hidden" name="client" value="{{client}}">
 <div class="form-group">
      <label >Projet / Centre de coût</label>
      <input type="text" class="form-control" name="project"  placeholder="Projet / Centre de coût">
    
    </div>
    <div class="form-group">
      <label >Chauffeur</label>
      <input type="text" class="form-control" name="chauffeur"  placeholder="Chauffeur">
    
    </div>
    <div class="form-group">
      <label>Matricule</label>
      <input type="text" class="form-control" name="matricule"  placeholder="Matricule">
    
    </div>
    <div class="form-group">
      <label >Volumes</label>
      <input type="text" class="form-control" name="volumes"  placeholder="Volumes">
    
    </div>
<div class="form-group">
    <div class="custom-control custom-switch">
      <input type="checkbox" class="custom-control-input" id="customSwitch1" name ="tva" checked="">
      <label class="custom-control-label" for="customSwitch1">Avec TVA</label>
    </div>
   
  </div> 

  <button type="submit" class="btn btn-primary">Confirmer</button>
 </form>
    